    "jsdom": "^20.0.0",
    "marked": "^2.0.1",
    "rollup-plugin-esbuild": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "6.0.0",
    "sass": "^1.53.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vite": "^2.9.13",
